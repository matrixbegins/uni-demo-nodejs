{
    "family": "uniDeployTaskDef",
    "executionRoleArn": "arn:aws:iam::665566361809:role/uniDeployECSTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "nodeJs-app",
            "image": "",
            "cpu": 1,
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "",
                    "value": ""
                }
            ],
            "startTimeout": 10,
            "stopTimeout": 10,

            "healthCheck": {
                "command": [
                    "echo \"yes\" "
                ],
                "interval": 0,
                "timeout": 0,
                "retries": 0,
                "startPeriod": 0
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ]
}
